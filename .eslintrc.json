{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "root": true,
  "rules": {
    "indent": ["error", 2],
    "semi": ["error", "never"],
    "@typescript-eslint/no-explicit-any": "off"
  }
}
